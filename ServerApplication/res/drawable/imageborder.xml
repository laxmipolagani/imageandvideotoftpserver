<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#848684"
                android:startColor="#BDBABD" />

            <stroke
                android:width="1dp"
                android:color="#313031" />

            <corners android:radius="10dp" />

            <padding
               
                android:left="10dp"
                android:right="10dp"
               />
        </shape>
    </item>

</layer-list>